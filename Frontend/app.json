{
  "expo": {
    "name": "Smart Hostel",
    "slug": "hms-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "hostelhub",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "ios",
      "android"
    ],
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "splash": {
      "backgroundColor": "#1d4ed8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hostelhub.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1d4ed8"
      },
      "package": "com.hostelhub.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.VIBRATE"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "@react-native-community/datetimepicker"
    ],
    "updates": {
      "url": "https://u.expo.dev/d0ca44c9-b72a-424f-88e5-ff0c4a1938b1"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {},
      "apiUrl": "https://hostel-management-system-backend-jde3.onrender.com/api",
      "eas": {
        "projectId": "d0ca44c9-b72a-424f-88e5-ff0c4a1938b1"
      }
    }
  }
}
